{
  "redirects": [
    { "source": "http://(.*)", "destination": "https://insomniastudio.fr/$1", "permanent": true },
    { "source": "https://www.insomniastudio.fr/(.*)", "destination": "https://insomniastudio.fr/$1", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Permissions-Policy", "value": "camera=(), microphone=(), geolocation=()" }
      ]
    }
  ]
}
